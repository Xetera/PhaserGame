{
    "rulesDirectory": [
        "./"
    ],
    "rules": {
        "indent": [true, "tabs"],
        "max-line-length": {
            "options": [
                180
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": {
            "options": [
                "debug",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}
