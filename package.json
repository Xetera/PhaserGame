    "url": "https://github.com/code0wl/Multiplayer-Phaser-game/issues"
  },
  "homepage": "https://github.com/code0wl/Multiplayer-Phaser-game#readme",
  "dependencies": {
    "colyseus": "^0.9.0",
    "colyseus.js": "^0.9.0",
    "express": "4.15.2",
    "phaser-ce": "^2.8.1",
    "serve-index": "^1.9.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.52",
    "@types/pixi.js": "^4.5.0",
