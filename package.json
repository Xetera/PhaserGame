    "fs-access": "^1.0.1",
    "graceful-readlink": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.2",
    "ignore-loader": "^0.1.2",
